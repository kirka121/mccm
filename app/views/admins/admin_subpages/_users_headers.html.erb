<table class="admin_panel_news_table">
	<tr>
		<th>#</th>
		<th>Name</th>
		<th>Email</th>
		<th>Created At</th>
		<th>Edit</th>
		<th>Delete</th>
	</tr>

	<% 
		counter = 0
		@users.each do |user| 
			counter+=1
	%>
			<tr>
				<td><%= counter %></td>
				<td><%= user.first_name.to_s + " " + user.last_name.to_s %></td>
				<td><%= user.email .to_s %></td>
				<td><%= user.created_at.to_s %></td>
				<td><%= link_to "edit", admin_subpages_edituser_path(:id => user.id)%></td>

				<td><%= link_to "delete", admin_subpages_deleteuser_path(:id => user.id), method: :delete,
                                  data: { confirm: "You sure?" }%></td>
			</tr>
	<% end %>
</table>
<br/>